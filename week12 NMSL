#include <stdio.h>
#include <stdlib.h>
#include <math.h>
void posfix(char Y[]);
char* infix_to_pre(char X[]);

int main(){
    char X[1000];
    scanf("%s",X);
    char Y[1000];
    Y = infix_to_pre(X)
    posfix(X);
    return 0;
}

void posfix(char Y[]){
    int stack[1000] = {0};
    int top = -1, j = 0, tmp;
    while(1){
        if( Y[j] >= 48 && Y[j] <= 57){
            top ++;
            stack[top] = Y[j] - 48;
        }else if( Y[j] == 42 || Y[j] == 43 || Y[j] == 45 || Y[j] == 47 || Y[j] == 94){
            tmp = stack[top];
            top --;
            stack[top] = ((Y[j] == '+')? stack[top] + tmp : (Y[j] == '-')?stack[top] - tmp : (Y[j] == '*')?stack[top] * tmp : (Y[j] == '^')? pow(stack[top],tmp) :stack[top] / tmp );
        }else{
            break;
        }
        j ++;
    }
    if(top == 0){
        printf("%d",stack[top]);
    }else{
        printf("\tThe expression isn't complete !\n");
    }
}

